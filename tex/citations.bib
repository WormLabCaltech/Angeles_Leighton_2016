year = {2015}
}
@article{Perez2007,
abstract = {T he backbone of scientific computing is mostly a collection of high-perfor-mance code written in Fortran, C, and C++ that typically runs in batch mode on large systems, clusters, and supercomputers. However, over the past decade, high-level environ-ments that integrate easy-to-use interpreted lan-guages, comprehensive numerical libraries, and visualization facilities have become extremely popu-lar in this field. As hardware becomes faster, the crit-ical bottleneck in scientific computing isn't always the computer's processing time; the scientist's time is also a consideration. For this reason, systems that allow rapid algorithmic exploration, data analysis, and vi-sualization have become a staple of daily scientific work. The Interactive Data Language (IDL) and Matlab (for numerical work), and Mathematica and Maple (for work that includes symbolic manipula-tion) are well-known commercial environments of this kind. GNU Data Language, Octave, Maxima and Sage provide their open source counterparts. All these systems offer an interactive command line in which code can be run immediately, without having to go through the traditional edit/com-pile/execute cycle. This flexible style matches well the spirit of computing in a scientific context, in which determining what computations must be performed next often requires significant work. An interactive environment lets scientists look at data, test new ideas, combine algorithmic approaches, and evaluate their outcome directly. This process might lead to a final result, or it might clarify how they need to build a more static, large-scale pro-duction code. As this article shows, Python (www.python.org) is an excellent tool for such a workflow. 1 The IPython project (http://ipython.scipy.org) aims to not only provide a greatly enhanced Python shell but also facilities for interactive distributed and par-allel computing, as well as a comprehensive set of tools for building special-purpose interactive envi-ronments for scientific computing.},
author = {P{\'{e}}rez, F. and Granger, B.E.},
doi = {doi:10.1109/MCSE.2007.53.},
file = {:Users/dangeles/Documents/Mendeley Library Copy/ipython07{\_}pe-gr{\_}cise.pdf:pdf},
isbn = {3518437208},
issn = {15219615},
journal = {Computing in Science and Engineering},
number = {3},
pages = {21--29},
pmid = {1000224768},
title = {{IPython: A System for Interactive Scientific Computing Python: An Open and General- Purpose Environment}},
url = {http://ipython.org},
volume = {9},
year = {2007}
}
@article{Waskom,
abstract = {This is a major release from 0.6. The main new feature is swarmplot which implements the beeswarm approach for drawing categorical scatterplots. There are also some performance improvements, bug fixes, and updates for compatibility with new versions of dependencies.$\backslash$n$\backslash$n$\backslash$n  Added the swarmplot function, which draws beeswarm plots. These are$\backslash$ncategorical scatterplots, similar to those produced by stripplot,$\backslash$nbut position of the points on the categorical axis is chosen to$\backslash$navoid overlapping points. See the$\backslash$ncategorical plot tutorial for more$\backslash$ninformation.$\backslash$n  Added an additional rule when determining category order in$\backslash$ncategorical plots. Now, when numeric variables are used in a$\backslash$ncategorical role, the default behavior is to sort the unique levels$\backslash$nof the variable (i.e they will be in proper numerical order). This$\backslash$ncan still be overridden by the appropriate {\{}*{\_}{\}}order parameter,$\backslash$nand variables with a category datatype will still follow the$\backslash$ncategory order even if the levels are strictly numerical.$\backslash$n  Changed some of the stripplot defaults to be closer to swarmplot.$\backslash$nPoints are somewhat smaller, have no outlines, and are not split by$\backslash$ndefault when using hue.$\backslash$n  Changed how stripplot draws points when using hue nesting with$\backslash$nsplit=False so that the different hue levels are not drawn$\backslash$nstrictly on top of each other.$\backslash$n  Improve performance for large dendrograms in clustermap.$\backslash$n  Added font.size to the plotting context definition so that the$\backslash$ndefault output from plt.text will be scaled appropriately.$\backslash$n  Fixed a bug in clustermap when fastcluster is not installed.$\backslash$n  Fixed a bug in the zscore calculation in clustermap.$\backslash$n  Fixed a bug in distplot where sometimes the default number of bins$\backslash$nwould not be an integer.$\backslash$n  Fixed a bug in stripplot where a legend item would not appear for a$\backslash$nhue level if there were no observations in the first group of$\backslash$npoints.$\backslash$n  Heatmap colorbars are now rasterized for better performance in$\backslash$nvector plots.$\backslash$n  Added workarounds for some matplotlib boxplot issues, such as$\backslash$nstrange colors of outlier points.$\backslash$n  Added workarounds for an issue where violinplot edges would be$\backslash$nmissing or have random colors.$\backslash$n  Added a workaround for an issue where only one heatmap cell would be$\backslash$nannotated on some matplotlib backends.$\backslash$n  Fixed a bug on newer versions of matplotlib where a colormap would$\backslash$nbe erroneously applied to scatterplots with only three observations.$\backslash$n  Updated seaborn for compatibility with matplotlib 1.5.$\backslash$n  Added compatibility for various IPython (and Jupyter) versions in$\backslash$nfunctions that use widgets.},
author = {Waskom, Michael and St-Jean, Samuel and Evans, Constantine and Warmenhoven, Jordi and Meyer, Kyle and Martin, Marcel and Rocher, Luc and Hobson, Paul and Bachant, Pete and Nagy, Tamas and Wehner, Daniel and Botvinnik, Olga and Megies, Tobias and Lukauskas, Saulius and Drewokane and Ziegler, Erik and Yarkoni, Tal and Miles, Alistair and Lee, Antony and Coelho, Luis Pedro and Halchenko, Yaroslav and Augspurger, Tom and Hitz, Gregory and Vanderplas, Jake and Fitzgerald, Clark and Cole, John B. and Gkunter and Villalba, Santi and Hoyer, Stephan and Quintero, Eric},
doi = {10.5281/zenodo.45133},
keywords = {Seaborn},
