issn = {1474-760X},
journal = {Genome biology},
keywords = {Animals,Caenorhabditis elegans,Caenorhabditis elegans: drug effects,Caenorhabditis elegans: genetics,Escherichia coli,Escherichia coli: genetics,Genes, Helminth,Genes, Helminth: genetics,Genetic Vectors,Genetic Vectors: administration {\&} dosage,Genetic Vectors: genetics,Microinjections,Phenotype,RNA, Double-Stranded,RNA, Double-Stranded: administration {\&} dosage,RNA, Double-Stranded: genetics,RNA, Helminth,RNA, Helminth: drug effects,RNA, Helminth: genetics,RNA, Helminth: metabolism,Transfection,Transfection: methods,Transformation, Genetic},
number = {1},
